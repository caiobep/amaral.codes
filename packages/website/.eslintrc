{
  "env": {
    "browser": true,
  },
  "extends": [
    "../.eslintrc",
    "plugin:astro/recommended"
  ],
  "plugins": [
    "astro"
  ],
  "overrides": [
    {
      "files": [
        "*.astro"
      ],
      "parser": "astro-eslint-parser",
      "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "extaFileExtensions": [
          ".astro"
        ]
      },
    }
  ]
}
